{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "prefer-const": "warn",
    "quotes": ["warn", "single"],
     "jsx-quotes": ["warn", "prefer-single"],
    "indent": ["warn", 4],
    "max-len": ["warn", {"code": 120}],
    "comma-dangle": ["warn", "never"],
    "semi": ["warn", "never"]
  }
}